{
  "name": "Cosm+",
  "description": "Unofficial Cosm app for Chrome",
  "manifest_version": 2,
  "version": "0.0.0.4",
  "app": {
    "launch": {
      "local_path": "index.html"
    }
  },
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "img/icon16.png",
    "24": "img/icon24.png",
    "32": "img/icon32.png",
    "112": "img/icon112.png",
    "128": "img/icon128.png"
  }
}